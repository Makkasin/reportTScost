																  и субОС.вид 		 = Значение(ПланВидовХарактеристик.ВидыСубконтоХозрасчетные.ОсновныеСредства)
																  и субОС.видДвижения = Значение(ВидДвиженияБухгалтерии.Дебет)
																				
INNER JOIN РегистрБухгалтерии.Хозрасчетный.Субконто КАК субСтз ON   субСтз.номерстроки = Хозрасчетный.номерстроки
																  и субСтз.Регистратор = Хозрасчетный.Регистратор
																  и субСтз.вид 		 = Значение(ПланВидовХарактеристик.ВидыСубконтоХозрасчетные.СтатьиЗатрат)
																  и субСтз.видДвижения = Значение(ВидДвиженияБухгалтерии.Дебет)
																				
INNER JOIN РегистрБухгалтерии.Хозрасчетный.Субконто КАК субНом ON   субНом.номерстроки = Хозрасчетный.номерстроки
																  и субНом.Регистратор = Хозрасчетный.Регистратор
																  и субНом.вид 		   = Значение(ПланВидовХарактеристик.ВидыСубконтоХозрасчетные.Номенклатура)
																  и субНом.видДвижения = Значение(ВидДвиженияБухгалтерии.Кредит)
																  

LEFT OUTER JOIN РегистрНакопления.ЗатратыДляЛимитов КАК ЗатратыДляЛимитов ON
							 (ЗатратыДляЛимитов.ОсновноеСредство = &amp;ФильтрТС или &amp;ФильтрТС = ЗНАЧЕНИЕ(Справочник.ОсновныеСредства.ПустаяССылка))
  							и ЗатратыДляЛимитов.Период &gt;= &amp;Начпер
  							и ЗатратыДляЛимитов.Период &lt;  &amp;КонПер
							и ЗатратыДляЛимитов.Регистратор = Хозрасчетный.Регистратор 													
																				
WHERE 

  Хозрасчетный.счетКт в ИЕРАРХИИ (Значение(ПланСчетов.Хозрасчетный.Материалы))
и Хозрасчетный.Период &gt;= &amp;Начпер
и Хозрасчетный.Период &lt;  &amp;КонПер
и (субОС.Значение = &amp;ФильтрТС или &amp;ФильтрТС = ЗНАЧЕНИЕ(Справочник.ОсновныеСредства.ПустаяССылка) )
и ЗатратыДляЛимитов.Регистратор IS NULL

GROUP BY
    CASE WHEN &amp;БезДок=TRUE THEN Неопределено ELSE Хозрасчетный.Регистратор END,
	Хозрасчетный.Период ,
	субОС.Значение  ,
	субНом.Значение ,
	субСтз.Значение ,
    Хозрасчетный.Регистратор


 ;	////////////////////////////////////////////////////
ВЫБРАТЬ
    Тбл.Регистратор,
    Тбл.Период Дата,
	тбПодр.Местонахождение Подразделение,
	СпрОС.ссылка КАК ОсновноеСредство,
	РегОС.УпрМодель Модель,
	РегОС.ГосНомер ГосНомер,
	РегОС.ТипТС ТипТС,
	ГОД(СпрОС.ДатаВыпуска) КАК ГодВыпуска,
	CASE WHEN ГОД(&amp;Конпер)-ГОД(СпрОС.ДатаВыпуска) &gt; 10  THEN "Свыше 10 лет" ELSE "До 10 лет" END КАК ВозрастТехники,
	СпрСтзГрп.ссылка КАК ГруппаЗатрат,
	СпрСтз.ссылка КАК СтатьяЗатрат,
	Тбл.Номенклатура КАК Номенклатура,
	СпрНом.Код КАК КодНом,
	Тбл.Аналитика КАК Аналитика,
	ТБл.СуммаЗЧ+СуммаУсл+СуммаТопливо КАК Сумма,
	ТБл.СуммаЗЧ КАК СуммаЗЧ,
	ТБл.СуммаТопливо КАК СуммаТопливо,
	ТБл.СуммаУсл КАК СуммаУсл,
	Тбл.Количество КАК Количество,
	тбЧасы.Часы,
	СпрОС.Код КАК ОсновноеСредствоКод,
	СпрСтз.Код КАК СтатьяЗатратКод,
	СпрСтзГрп.Код КАК ГруппаЗатратКод
ИЗ
	врЗтр Тбл
LEFT OUTER JOIN  ВРПодр тбПодр ON тбПодр.ТС        =  Тбл.ОсновноеСредство 
								и тбПодр.Период    &lt;= Тбл.Период
								и тбПодр.ПериодКон &gt;  Тбл.Период
								
INNER JOIN Справочник.ОсновныеСредства СпрОС ON СпрОС.ссылка = Тбл.ОсновноеСредство								
LEFT OUTER JOIN РегистрСведений.ДопРеквизитыОС РегОС ON РегОС.ОС = ТБл.ОсновноеСредство
								
LEFT OUTER JOIN Справочник.СтатьиЗатрат СпрСтз ON СпрСтз.ссылка = Тбл.СтатьяЗатрат

LEFT OUTER JOIN Справочник.СтатьиЗатрат СпрСтзГрп ON СпрСтзГрп.Код = Подстрока(СпрСтз.Код,1,3)

LEFT OUTER JOIN Справочник.Номенклатура СпрНом ON СпрНом.ссылка = Тбл.Номенклатура

LEFT OUTER JOIN врЧасы тбЧасы ON тбЧасы.ОС = ТБл.ОсновноеСредство

WHERE (&amp;фильтрДорогиеЗЧ = Ложь или СуммаЗЧ/CASE WHEN Количество=0 THEN 1 ELSE Количество END &gt;25000)</query>
