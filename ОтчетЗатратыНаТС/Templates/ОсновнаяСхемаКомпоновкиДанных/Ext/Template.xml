		<dataSource>ИсточникДанных1</dataSource>
		<query>ВЫБРАТЬ
		уатМестонахождениеТССрезПоследних.ОсновноеСредство ТС,
		уатМестонахождениеТССрезПоследних.Местонахождение ,
		уатМестонахождениеТССрезПоследних.Период
		INTO ВРТбл
	ИЗ
		РегистрСведений.МестонахождениеОСБухгалтерскийУчет.СрезПоследних(&amp;НачПер,ОсновноеСредство = &amp;ФильтрТС или &amp;ФильтрТС = ЗНАЧЕНИЕ(Справочник.ОсновныеСредства.ПустаяССылка)) КАК уатМестонахождениеТССрезПоследних
		
	UNION ALL
		
	ВЫБРАТЬ
		уатМестонахождениеТС.ОсновноеСредство ТС,
		уатМестонахождениеТС.Местонахождение ,
		уатМестонахождениеТС.Период
	ИЗ
		РегистрСведений.МестонахождениеОСБухгалтерскийУчет КАК уатМестонахождениеТС
		WHERE Период &gt; &amp;НачПер
		    и Период &lt; &amp;КонПер
		    и (ОсновноеСредство = &amp;ФильтрТС или &amp;ФильтрТС = ЗНАЧЕНИЕ(Справочник.ОсновныеСредства.ПустаяССылка))
	;
		
	SELECT  DISTINCT
		Тбл.ТС,
		Тбл.Местонахождение,
		Тбл.Период,
		MIN(ISNULL(Т.Период,&amp;КонПер)) ПериодКон,
		Тбл.Период изначДт
	INTO ВРТбл1	
	FROM ВРТбл Тбл 
	LEFT OUTER JOIN ВРТбл Т ON Т.ТС = Тбл.ТС и Т.Период &gt; Тбл.Период
	GROUP BY		
		Тбл.ТС,
		Тбл.Местонахождение,
		Тбл.Период
	;	
	SELECT DISTINCT
		Тбл.ТС,
		Тбл.Местонахождение,
		Тбл.Период,
		Тбл.ПериодКон
	INTo ВРПодр
	FROM ВРТбл1 Тбл           
	INNER JOIN (SELECT ТС,Период,MAX(изначДт) изНачДт FROM ВРТбл1 Т1 GROUP BY ТС,Период) Т ON Т.ТС = Тбл.ТС и Т.Период = ТБл.Период и Т.ИзначДт = Тбл.ИзначДт
	
	;
	
	////////////////////////////////////////////////////
	SELECT
	    ОсновноеСредство ОС,
	    SUM(Часы) Часы
	    
	    INTO врЧасы
	  FROM РегистрНакопления.ДоходыТС Рег
	  WHERE   Период &gt;= &amp;НачПер
	  		и Период &lt;  &amp;КонПер
	  		и (ОсновноеСредство = &amp;ФильтрТС или &amp;ФильтрТС = ЗНАЧЕНИЕ(Справочник.ОсновныеСредства.ПустаяССылка)) 
	  GROUP BY  ОсновноеСредство
	 ;
	////////////////////////////////////////////////////
//Лимиты
ВЫБРАТЬ
    CASE WHEN &amp;БезДок=TRUE THEN Неопределено ELSE Регистратор END Регистратор,
	ЗатратыДляЛимитов.Период КАК Период,
	ЗатратыДляЛимитов.ОсновноеСредство КАК ОсновноеСредство,
	ЗатратыДляЛимитов.Номенклатура КАК Номенклатура,
	ЗатратыДляЛимитов.СтатьяЗатрат КАК СтатьяЗатрат,
	ЗатратыДляЛимитов.Количество КАК Количество,
	ЗатратыДляЛимитов.Сумма КАК СуммаЗЧ,
	ЗатратыДляЛимитов.СуммаТопливо КАК СуммаТопливо,
	0 КАК СуммаУсл,
	ЗатратыДляЛимитов.ДокументОснование КАК Аналитика
	
INTO врЗтр
ИЗ
	РегистрНакопления.ЗатратыДляЛимитов КАК ЗатратыДляЛимитов
ГДЕ 
	(ЗатратыДляЛимитов.ОсновноеСредство = &amp;ФильтрТС или &amp;ФильтрТС = ЗНАЧЕНИЕ(Справочник.ОсновныеСредства.ПустаяССылка))
  и ЗатратыДляЛимитов.Период &gt;= &amp;Начпер
  и ЗатратыДляЛимитов.Период &lt;  &amp;КонПер


UNION ALL

///сч 60
ВЫБРАТЬ
    CASE WHEN &amp;БезДок=TRUE THEN Неопределено ELSE Хозрасчетный.Регистратор END Регистратор,
	Хозрасчетный.Период КАК Период,
	субОС.Значение  КАК ОС,
	Хозрасчетный.Содержание КАК Номенклатура,
	субСтз.Значение КАК Стз,
	Хозрасчетный.КоличествоКт КАК КоличествоКт,
	0 КАК СуммаЗЧ,
	0 КАК СуммаТопливо,
	SUM(Хозрасчетный.Сумма) КАК СуммаУсл,
	ISNULL(СпрКА.Наименование,"&lt;Не указан контрагент&gt;")  КАК Аналитика

ИЗ
	РегистрБухгалтерии.Хозрасчетный КАК Хозрасчетный
INNER JOIN ПланСчетов.Хозрасчетный ПланСчт                     ON   ПланСчт.ссылка = Хозрасчетный.счетДт и ПланСчт.Код = "22" 
INNER JOIN РегистрБухгалтерии.Хозрасчетный.Субконто КАК субОС  ON   субОС.номерстроки = Хозрасчетный.номерстроки
																  и субОС.Регистратор = Хозрасчетный.Регистратор
																  и субОС.вид 		 = Значение(ПланВидовХарактеристик.ВидыСубконтоХозрасчетные.ОсновныеСредства)
																  и субОС.видДвижения = Значение(ВидДвиженияБухгалтерии.Дебет)
																				
INNER JOIN РегистрБухгалтерии.Хозрасчетный.Субконто КАК субСтз ON   субСтз.номерстроки = Хозрасчетный.номерстроки
																  и субСтз.Регистратор = Хозрасчетный.Регистратор
																  и субСтз.вид 		 = Значение(ПланВидовХарактеристик.ВидыСубконтоХозрасчетные.СтатьиЗатрат)
																  и субСтз.видДвижения = Значение(ВидДвиженияБухгалтерии.Дебет)
																				
INNER JOIN РегистрБухгалтерии.Хозрасчетный.Субконто КАК субКА ON    субКА.номерстроки = Хозрасчетный.номерстроки
																  и субКА.Регистратор = Хозрасчетный.Регистратор
																  и субКА.вид 		 = Значение(ПланВидовХарактеристик.ВидыСубконтоХозрасчетные.Контрагенты)
																  и субКА.видДвижения = Значение(ВидДвиженияБухгалтерии.Кредит)
																  
LEFT OUTER JOIN Справочник.Контрагенты СпрКА 					  ON  СпрКА.ссылка = субКА.Значение
																				
WHERE 

  Хозрасчетный.счетКт в  (Значение(ПланСчетов.Хозрасчетный.РасчетыСПоставщиками))
и Хозрасчетный.Период &gt;= &amp;Начпер
и Хозрасчетный.Период &lt;  &amp;КонПер
и (субОС.Значение = &amp;ФильтрТС или &amp;ФильтрТС = ЗНАЧЕНИЕ(Справочник.ОсновныеСредства.ПустаяССылка) )

GROUP BY  
    CASE WHEN &amp;БезДок=TRUE THEN Неопределено ELSE Хозрасчетный.Регистратор END,
    Хозрасчетный.Период ,
	субОС.Значение  ,
	Хозрасчетный.Содержание,
	субСтз.Значение,
	Хозрасчетный.КоличествоКт,
    ISNULL(СпрКА.Наименование,"&lt;Не указан контрагент&gt;")

UNION ALL


